{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LoadTestSettings": {
    "TargetUrl": "https://your-api-url/api", 
    "Concurrency": 50, // Number of concurrent users
    "RequestRate": 200, // Requests per second
    "Duration": "5m" // Duration of the test, e.g., 5 minutes
  },
  "AllowedHosts": "*"
}
